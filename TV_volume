#include <iostream>

using namespace std;

int main()
{
    int t,v,v1,v2;
    cout<<"enter test cases:";
    cin>>t;
    cout << "enter volume:" << endl;
    cin>>v;
    while(t--){
        cout<<"\nenter volume to be updated:";
            cin>>v1;
            if(v>v1)
                v2=v-v1;
            else v2=v1-v;
            cout<<"number of times the button has to be pressed is:"<<v2;
    }
    return 0;
}
   
